function Analyze(x,u,t)
    figure(1)
    clf(1)
    for i = 1:size(x,1)

       axis([-2,2,-1.5,1.5])
       title(t(i))
       line([x(i,1), x(i,1) - sin(x(i,3))],[0,cos(x(i,3))])
       hold on
       %line([x(i,5), x(i,5) - sin(x(i,7))],[0,cos(x(i,7))])
       
       grid on
       plot(x(i,1),0,'o')
       %plot(x(i,5),0,'x')
       pause(0.01)
       if (abs(x(i,1))<0.01) && (abs(x(i,2))<0.01)
           break;
       end
       clf(1)
    end
    name = ['Cart ', ]
    figure(2)
    clf(2)

end

